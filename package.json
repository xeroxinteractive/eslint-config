{
  "name": "@xerox/eslint-config",
  "version": "1.4.3",
  "description": "ESLint shareable configuration for Xerox projects.",
  "main": "index.js",
  "repository": "git@github.com:xeroxinteractive/eslint-config.git",
  "author": "Andrew Leedham <andrew.leedham@xerox.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "deploy": "semantic-release",
    "test": "jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@xerox/prettier-config",
  "eslintConfig": {
    "extends": [
      "./index.js"
    ]
  },
  "release": {
    "extends": "@xerox/semantic-release-config/npm"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@xerox/prettier-config": "^1.0.0",
    "@xerox/semantic-release-config": "^1.4.0",
    "eslint": "^6.5.1",
    "husky": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.16.4",
    "semantic-release": "^15.13.3",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-eslint": "^10.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest": "^23.0.0",
    "eslint-plugin-jsdoc": "^17.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.12.4"
  },
  "peerDependencies": {
    "eslint": "~6",
    "prettier": "~1"
  },
  "resolutions": {
    "handlebars": ">=4.3.0",
    "set-value": ">=2.0.1",
    "mem": ">=4.0.0"
  }
}
