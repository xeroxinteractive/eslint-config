{
  "name": "@xerox/eslint-config",
  "version": "1.8.0",
  "description": "ESLint shareable configuration for Xerox projects.",
  "main": "index.js",
  "repository": "git@github.com:xeroxinteractive/eslint-config.git",
  "author": "Andrew Leedham <andrew.leedham@xerox.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "deploy": "semantic-release"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@xerox/prettier-config",
  "eslintConfig": {
    "extends": [
      "./index.js"
    ]
  },
  "release": {
    "extends": "@xerox/semantic-release-config/npm"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@xerox/prettier-config": "^1.0.0",
    "eslint": "^5.16.0",
    "husky": "^3.0.0",
    "prettier": "^1.16.4",
    "semantic-release": "^15.13.3",
    "@xerox/semantic-release-config": "^1.4.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-eslint": "^10.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsdoc": "^15.3.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4"
  },
  "peerDependencies": {
    "eslint": "^5.16.0",
    "prettier": ">=1",
    "typescript": "^3.5.1"
  },
  "resolutions": {
    "lodash": ">=4.16.13",
    "mixin-deep": ">=1.3.2",
    "set-value": ">=2.0.1",
    "mem": ">=4.0.0"
  }
}
